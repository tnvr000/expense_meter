<%= form_with(model: expense) do |f| %>
  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <div>
    <%= f.label :amount %>
    <%= f.text_field :amount %>
  </div>
  <div>
    <%= f.label :date_created_on, "Date" %>
    <%= f.date_field :date_created_on %>
  </div>
  <%= f.submit %>
<% end %>